page{
  background: #F8F8F8;
}

.search-container{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
}

.content{
  width: 100%;
  display: flex;
  flex-direction: column;
}

.search-container{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.search-container .search{
  width: 95%;
  display: flex;
  flex-direction: row;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
}

.search .search-input{
  background: #E6E6E6;
  width:85%;
  height: 75rpx;
  font-weight: 200;
  font-size: 30rpx;
  text-align: center;
  border-radius:15rpx 0 0 15rpx;
}

.search-image{
  width: 15%;
  background: white;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  border-radius: 0 15rpx 15rpx 0;
}

.search-image image{
  width: 40rpx;
  height: 40rpx;
}

.result-content{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.result{
  width: 95%;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
}

.result-item{
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-bottom: 50rpx;
}

.title{
  border-left-style:solid;
  border-width:8rpx;
  border-color: #3CC4A9;
  font-size: 35rpx;
  padding: 0rpx 10rpx;
}

.sub-item{
  width: 100%;
  margin-top: 15rpx;
  font-size: 30rpx;
}

.item{
  margin-left: 30rpx;
  padding-bottom: 10rpx;
}

.not-data{
  width: 100%;
  padding: 10rpx 0rpx;
  text-align: center;
  color: darkgray;
  font-size: 30rpx;
}

.huishou-c{
  color: #215287;
}

.youhai-c{
  color: #EA3E30;
}

.si-c{
  color: #62423A;
}

.gan-c{
  color: #2E2C2A;
}